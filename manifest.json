{
  "name": "SoContact : Get Phone Numbers and Emails",
  "version": "1.0.0.3",
  "description": "Get your prospectsâ€™ phone number and email instantly from LinkedIn. Convert leads with and automate your prospecting.",
    "default_locale": "en",
  "author": "Nasirul Akbar Khan",
  "action": {
    "default_icon":{
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png",
      "48": "assets/icons/icon48.png",
      "64": "assets/icons/icon64.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "64": "assets/icons/icon64.png",
    "128": "assets/icons/icon128.png"
  },
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "content_scripts": [ 
    {
      "all_frames": false,
      "js": ["assets/js/libs/jquery.js","assets/js/common.js","assets/js/popup.js"],
      "matches": ["*://*.linkedin.com/*","*://linkedin.com/*"],
      "run_at": "document_end"
    },
    {
      "all_frames": false,
      "js": ["assets/js/libs/jquery.js","assets/js/libs/moment.min.js","assets/js/common.js","assets/js/content.js"],
      "matches": ["*://*.linkedin.com/*","*://linkedin.com/*"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3,
  "web_accessible_resources":[{
    "resources": ["assets/css/style.css", "assets/icons/*", "assets/img/*"],
    "matches": ["*://*.linkedin.com/*","*://linkedin.com/*"]
  }],
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://app.socontact.com/"
  ]
}